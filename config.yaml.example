# Check the tiralibConfig class in tiralib/utils/config.py for more information about the config file

tiralib:
  workspace: "workspace"

tiramisu: 
  is_new_tiramisu: False

env_vars:
  CXX: "${CXX}"
  CC: "${CC}"
  TIRAMISU_ROOT: "path/to/tiramisu"
  LD_LIBRARY_PATH: "${TIRAMISU_ROOT}/3rdParty/Halide/build/src:${TIRAMISU_ROOT}/3rdParty/llvm/build/lib:${TIRAMISU_ROOT}/build:${TIRAMISU_ROOT}/3rdParty/isl/build/lib"
  TIRAMISU_HERMESII_PATH: "path/to/hermesii/installation"
  CONDA_ENV: "path/to/conda/env"